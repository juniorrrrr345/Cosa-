{
  "version": 2,
  "name": "cosa-tau",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_URL": "https://cosa-tau.vercel.app"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["fra1"],
  "rewrites": [
    {
      "source": "/panel",
      "destination": "/panel"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}